{
  "version": "1.0.11",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint src test --fix",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "np": "np"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint --fix"
    }
  },
  "name": "zod-rosetty",
  "author": "qlaffont",
  "repository": {
    "type": "git",
    "url": "https://github.com/qlaffont/zod-rosetty.git"
  },
  "bugs": {
    "url": "https://github.com/qlaffont/zod-rosetty/issues",
    "email": "contact@qlaffont.fr"
  },
  "module": "dist/zod-rosetty.esm.js",
  "size-limit": [
    {
      "path": "dist/zod-rosetty.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/zod-rosetty.esm.js",
      "limit": "10 KB"
    }
  ],
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "src/index.ts"
    ]
  },
  "devDependencies": {
    "@fastify/swagger": "^8.7.0",
    "@fastify/swagger-ui": "^1.9.2",
    "@jest/globals": "29.6.2",
    "@size-limit/preset-small-lib": "8.2.6",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "husky": "8.0.3",
    "jest": "^29.5.0",
    "prettier": "3.0.1",
    "rosetty": "^1.4.137",
    "size-limit": "8.2.6",
    "ts-jest": "^29.0.5",
    "tsdx": "0.14.1",
    "tslib": "2.6.1",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "zod": "^3.21.4"
  }
}
